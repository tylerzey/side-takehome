{
  "name": "@side/app",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "dotenv -- turbo run build",
    "codegen": "dotenv -- turbo run codegen",
    "deploy": "dotenv -- turbo run deploy",
    "lint": "bash ./scripts/lint.sh",
    "prettier": "prettier --write \"**/*.{ts,js,tsx,json,md,yml,js,css}\" $@",
    "security-check": "bash ./scripts/security.sh",
    "sort-pkc": "sort-package-json 'package.json' 'apps/*/package.json' 'packages/*/package.json'",
    "test": "dotenv -- turbo run test -- --run",
    "type-check": "turbo run type-check"
  },
  "resolutions": {},
  "dependencies": {
    "@casl/ability": "6.5.0",
