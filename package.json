    "axios-retry": "4.3.0",
    "dataloader": "2.2.2",
    "dayjs": "1.11.11",
    "dotenv-cli": "7.3.0",
    "graphql": "16.8.1",
    "lodash": "4.17.21",
    "p-limit": "5.0.0",
    "p-retry": "6.2.0",
    "qs": "6.11.2",
    "source-map-support": "0.5.21",
    "turbo": "1.11.2",
    "uuid": "9.0.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "9.3.0",
